<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="playlistBox"
      fx:controller="app.musicplayer.controllers.PlaylistBoxController"
      stylesheets="@/assets/ui/css/PlaylistBox.css"
      styleClass="sideBarItem"
      minWidth="50.0"
      prefHeight="50.0"
      prefWidth="250.0"
      xmlns:fx="http://javafx.com/fxml/1">

   <Label fx:id="playlistTitleLabel"
          styleClass="playlist-title-label"
          prefHeight="30.0"
          textOverrun="CLIP">

      <HBox.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </HBox.margin>
   </Label>
</HBox>
